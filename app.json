{
  "expo": {
    "name": "Blood Donation",
    "slug": "blooddonation",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Blood Donation Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Blood Donation Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.blooddonation.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Blood Donation Logo.png",
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "096a1b85-f188-4585-82a4-93ce05ec1977"
      }
    },
    "owner": "mdriads-organization"
  }
}
